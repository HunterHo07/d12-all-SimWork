# SimWork Environment Variables Example
# Copy this file to .env and modify as needed

# Base URL for development and production
NEXT_PUBLIC_BASE_URL=http://localhost:3000
NEXT_PUBLIC_PRODUCTION_URL=https://yourusername.github.io/SimWork

# API Keys (replace with your actual keys in .env)
NEXT_PUBLIC_OPENAI_API_KEY=your_openai_api_key
NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id

# Feature Flags
NEXT_PUBLIC_ENABLE_AI_ASSISTANCE=true
NEXT_PUBLIC_ENABLE_VOICE_COMMANDS=false
